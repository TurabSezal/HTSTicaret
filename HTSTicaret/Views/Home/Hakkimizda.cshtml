    <script>
        function initMap() {
            // Haritanın gösterileceği div'i belirtin
            var mapDiv = document.getElementById('map');

            // Haritanın başlangıç koordinatlarını belirtin
            var map = new google.maps.Map(mapDiv, {
                center: { lat: 38.491511, lng: 27.706301 },
                zoom: 18,
                // Haritanın yüksekliğini ve genişliğini belirtin
                height: 600,
                width: 800
            });
        }
    </script>
